import java.util.Scanner;
public class Test05 
{
	public static void main(String[] args) 
	{
		Scanner in=new Scanner(System.in);
		System.out.print("输入三个整数：");
		int a=in.nextInt();
		int b=in.nextInt();
		int c=in.nextInt();
		int t;
		if(a>b){
			t=a;
			a=b;
			b=t;
		}
		if(a>c){
			t=a;
			a=c;
			c=t;
		}
		if(b>c){
			t=b;
			b=c;
			c=t;
		}
	}
}
